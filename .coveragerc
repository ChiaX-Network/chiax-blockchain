[run]
branch=True
relative_files=True
source_pkgs=
  xxch
  tests
omit=
  tests/**/config.py
concurrency=multiprocessing, thread
parallel=True

[paths]
source =
  xxch/
  venv/**/site-packages/xxch/

[report]
precision = 1
exclude_lines =
  pragma: no cover
  abc\.abstractmethod
  typing\.overload
  ^\s*\.\.\.\s*$
  if typing.TYPE_CHECKING:
  if TYPE_CHECKING:
